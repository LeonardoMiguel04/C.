// Peça ao usuario um numero inteiro e imprima na tela a tabuada
#include <stdio.h>

int main() {
    int n;
    int cont = 1;
    int m;
    
    printf("Digite o número para a tabuada: ");
    scanf(" %d", &n);
    
    while(cont <= 10){
        m = n*cont;
        printf(" %d", m);
        cont = cont + 1;
    }
    

    return 0;
}
